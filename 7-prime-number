public Boolean isPrime(Integer num) {
    if (num == 2){
        return true;
    }
    else if (Math.mod(num,2)==0){
        return false;
    }
    else{
        for (Integer i = 1; i < num; i++){
            if (Math.mod(num,i)==0){
                return true;
            }
        }
    
        return false;
    }
}