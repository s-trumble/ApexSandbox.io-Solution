public Boolean isPrime(Integer num) {
    if (num == 2){
        return true;
    }
    else if (Math.mod(num,2)==0){
        return false;
    }
    else{
        Integer i = 1;
        while (i < num){
            if (Math.mod(num,i)==0){
                return true;
            }
            else {
            i = i + 1;
            }
        }
        return false;
    }
}