public Boolean isPrime(Integer num) {
    if (num == 2){
        return true;
    }
    else if (Math.mod(num,2)==0){
        return false;
    }
    else{
        Integer int = 1;
        while (int < num){
            if (Math.mod(num,int)==0){
                return true;
            }
            else {
            int = int + 1;
            }
        }
        return false;
    }
}