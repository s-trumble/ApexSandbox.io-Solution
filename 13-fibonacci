public Integer fibonacci(Integer n) {
    //code here
    //List<Integer> numbers = new List<Integer>{1,1};
    Integer a = 1;
    Integer b = 1;
    Integer c;
    Integer fib;

    if (n ==1 || n==2){
        return a;
    }
    else {

    
    for(Integer i = 3; i <= n; i++ ){
            c = a + b;
            fib = c; 
            a=b;
            b=c;
            
        }        
    }
    return fib;
}