public Integer rockPaperScissors(String player1, String player2) {
    
    if (player1 == player2){
        // same choice
        return 0;
    }
    else if (
        (player1 == 'rock' && player2 == 'scissors') ||
        (player1 == 'scissors' && player2 == 'paper') ||
        (player1 == 'paper' && player2 == 'rock')
        ){
       //player 1 wins
       return 1;
        }

    else {
        // player 2 wins
        return 2;
        }

}